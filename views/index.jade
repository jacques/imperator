extends layout

block content
	h1 Index

	if user
		h3 Routes
		dl
			dd
				a(href='/cf/personas') cfEngine Personas
			dd
				a(href='/network') Networks
		dl
			dd
				a(href='/environment') Environments
			dd
				a(href='/platform') Platforms
			dd
				a(href='/tier') Tiers
			dd
				a(href='/machine') Machines
			dd
				a(href='/stingray') Stingrays

	if user
		dl
			dt Id
			dd= user.id
			dt Username
			dd= user.username
			dt Password
			dd= user.password
